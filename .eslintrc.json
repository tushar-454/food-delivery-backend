{
  "extends": ["prettier", "airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "env": {
    "commonjs": true,
    "node": true
  },
  "rules": {
    "no-console": "off",
    "object-curly-newline":"off",
    "operator-linebreak":"off",
    "no-extranaous-dependencies":"off",
  },
  "plugins": ["prettier"]
}
